<formcell 
	className="{{cls}}" 
	style="{{style}}"
	label="{{labelVisible ? label : null}}"
	layout="{{layout}}"
	multiCell="{{false}}"
	requiredFlag="{{requiredFlag}}"
>
	<view class="{{subCls}}">
		<view class="weui-cell__bd">
			<picker
			 class="weui-picker__group"
			 bindchange="onChange"
			 bindcancel="onCancel"
			 mode="{{mode}}"
			 range="{{selectRange}}"
			 start="{{start}}"
			 end="{{end}}"
			 value="{{value}}"
			>
				<label class="{{displayCls}}">{{displayValue}}</label>
			</picker>
		</view>
	</view>
</formcell>